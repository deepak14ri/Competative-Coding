def subs(s,n):
    c = 0
    t = []
    for i in range(n):
        for j in range(i+1,n+1):
            a = s[i:j]
            t.append(a)
            if a==s:
                print(c+1)
            else:
                c = c+1
    print(t)
s = input()
subs(s,len(s))



'''input: abcd
   output: 4
   
   '''
